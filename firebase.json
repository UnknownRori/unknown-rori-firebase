{
    "hosting": {
        "public": "build",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "/.well-known/discord",
                "destination": "/.well-known/discord"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}